{
    "IP Address" : {
        "rule": [{"TEXT": {"REGEX": "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}$"}}],
        "IOB" : "IPADDR"
    },
    "MAC Address" : {
        "rule" : [{"TEXT": {"REGEX": "^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})|([0-9a-fA-F]{4}\\.[0-9a-fA-F]{4}\\.[0-9a-fA-F]{4})$"}}],
        "IOB" : "MACADDR"
    },
    "Root User" : {
        "rule" : [{"TEXT": "root"}],
        "IOB" : "ROOT"
    },
    "Proccess Identifier" : {
        "rule" : [{"TEXT": "pid"}, {"IS_PUNCT": true}, {"IS_DIGIT" : true}],
        "IOB" : "PID"
    },
    "Authentication Server" : {
        "rule" : [{"TEXT": "sshd"}],
        "IOB" : "SSHD"
    },
    "CRON Process" : {
        "rule" : [{"TEXT": "CRON"}],
        "IOB" : "CRON"
    },
    "Port" : {
        "rule" : [{"TEXT" : "port"}, {"IS_DIGIT" : true}],
        "IOB" : "PORT"
    },
    "Sudo Command" : {
        "rule" : [{"TEXT" : "sudo"}],
        "IOB" : "SUDO"
    },
    "Kernel Proccess" : {
        "rule" : [{"IS_PUNCT" : true}, {"TEXT" : "kernel"}, {"IS_PUNCT" : true}],
        "IOB" : "KERNL"
    },
    "Website" : {
        "rule" : [{"LIKE_URL": true}],
        "IOB" : "WEBSITE"
    },
    "E-mail" : {
        "rule" : [{"LIKE_EMAIL": true}],
        "IOB" : "EMAIL"
    },
    "Authentication Process" : {
        "rule" : [{"TEXT": {"REGEX": "^pam_unix"}}, {"IS_PUNCT": true}, {"TEXT": "auth"}, {"IS_PUNCT": true}], 
        "IOB" : "AUTHPROC"
    },

    "Session Process" : {
        "rule" : [{"TEXT": {"REGEX": "^pam_unix"}}, {"IS_PUNCT": true}, {"TEXT": "session"}, {"IS_PUNCT": true}], 
        "IOB" : "SESSPROC"
    }
}