{"id": "IP Address", "label" : "IPADDR", "pattern": [{"TEXT": {"REGEX": "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}$"}}]}
{"id" : "MAC Address", "label" : "MACADDR", "pattern" : [{"TEXT": {"REGEX": "^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})|([0-9a-fA-F]{4}\\.[0-9a-fA-F]{4}\\.[0-9a-fA-F]{4})$"}}]}
{"id" : "Root User", "label" : "ROOT", "pattern" : [{"TEXT": "root"}]}
{"id" : "Process Identifier", "label" : "PID", "pattern": [{"TEXT": "pid"}, {"IS_PUNCT": true}, {"IS_DIGIT" : true}]}
{"id" : "Authentication Server", "pattern" : [{"TEXT": "sshd"}], "label" : "SSHD"}
{"id" : "CRON Process", "pattern" : [{"TEXT": "CRON"}], "label" : "CRON"}
{"id" : "Port", "label" : "PORT", "pattern" : [{"TEXT" : "port"}, {"IS_DIGIT" : true}]}
{"id" : "Sudo Command", "label" : "SUDO", "pattern" : [{"TEXT" : "sudo"}] }
{"id" : "Kernel Proccess", "label" : "KERNL", "pattern" : [{"IS_PUNCT" : true}, {"TEXT" : "kernel"}, {"IS_PUNCT" : true}] }
{"id" : "Website", "label" : "WEBSITE", "pattern" : [{"LIKE_URL": true}] }
{"id" : "E-mail", "label" : "EMAIL", "pattern" : [{"LIKE_EMAIL": true}]}
{"id" : "Authentication Process",  "label" : "AUTHPROC", "pattern" : [{"TEXT": {"REGEX": "^pam_unix"}}, {"IS_PUNCT": true}, {"TEXT": "auth"}, {"IS_PUNCT": true}]}
{"id" : "Session Process", "label" : "SESSPROC", "pattern" : [{"TEXT": {"REGEX": "^pam_unix"}}, {"IS_PUNCT": true}, {"TEXT": "session"}, {"IS_PUNCT": true}]}

